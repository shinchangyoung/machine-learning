sample_df = {
    'var_1': [2, 4, 5, 1, 4, 5, 7, 8, 2, 9],
    'var_2': [2, 3, 4, 4, 5, 4, 5, 8, 3, 3],
    'var_3': [1, 3, 6, 5, 7, 8, 2, 1, 5, 7],
    'var_4': [4, 7, 3, 6, 8, 9, 0, 7, 2, 6],
    'var_5': [3, 1, 5, 7, 3, 4, 6, 8, 1, 5]
}

# 인덱스 설정
index = ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j']

# DataFrame을 생성
import pandas as pd
df = pd.DataFrame(sample_df, index=index)

# 출력
print(df)
